{
  "name": "homebridge-volvo-ex30",
  "displayName": "Volvo EX30",
  "version": "2.1.6",
  "description": "Comprehensive Homebridge plugin for Volvo EX30 with Connected Vehicle API v2 support - battery monitoring, door/window sensors, lock control, climate control, and diagnostics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "npm run build && npm run watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "oauth-setup": "node scripts/oauth-setup.js"
  },
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "volvo",
    "ex30",
    "homekit",
    "smart-car"
  ],
  "author": "James Field",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "homebridge": ">=1.6.0"
  },
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^1.0.3",
    "axios": "^1.6.0",
    "node-cache": "^5.1.2",
    "openid-client": "^6.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "homebridge": "^1.6.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "homebridge": ">=1.6.0"
  },
  "homebridge": {
    "customUI": true
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jcfield-boop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcfield-boop/homebridge-volvoEX30.git"
  },
  "bugs": {
    "url": "https://github.com/jcfield-boop/homebridge-volvoEX30/issues"
  },
  "homepage": "https://github.com/jcfield-boop/homebridge-volvoEX30#readme",
  "files": [
    "dist/**/*",
    "src/**/*",
    "homebridge-ui/**/*",
    "scripts/**/*",
    "config.schema.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ]
}
